+ *--------------------------------------------------------------------------------------------*/
+
+import { Disposable } from '../../../../base/common/lifecycle.js';
+import { IWorkbenchContribution } from '../../../common/contributions.js';
+import { INotificationService, NotificationsFilter } from '../../../../platform/notification/common/notification.js';
+import { IExtensionService } from '../../../services/extensions/common/extensions.js';
+import { ILogService } from '../../../../platform/log/common/log.js';
+
+/**
+ * This contribution filters notifications from third-party extensions
+ * to reduce notification noise from non-WSO2 extensions.
+ */
+export class WSO2ThirdPartyNotificationFilter extends Disposable implements IWorkbenchContribution {
+
+	static readonly ID = 'workbench.contrib.wso2ThirdPartyNotificationFilter';
+
+	// List of WSO2 extension IDs that should NOT be filtered
+	private static readonly ALLOWED_EXT_ORGS = [
+		'wso2',
+	];
+
+	constructor(
+		@INotificationService private readonly notificationService: INotificationService,
+		@IExtensionService private readonly extensionService: IExtensionService,
+		@ILogService private readonly logService: ILogService,
+	) {
+		super();
+
+		// Wait for extensions to be registered before applying filters
+		this.extensionService.whenInstalledExtensionsRegistered().then(() => {
+			this.applyNotificationFilters();
+		});
+	}
+
+	private applyNotificationFilters(): void {
+		try {
+			const extensions = this.extensionService.extensions;
+			
+			for (const extension of extensions) {
+				const extensionId = extension.identifier.value;
+				
+				// Skip built-in extensions and WSO2 extensions
+				if (extension.isBuiltin || this.isWSO2Extension(extensionId)) {
+					continue;
+				}
+
+				// Filter notifications from third-party extensions
+				// Set filter to ERROR to only show error notifications
+				this.notificationService.setFilter({
+					id: extensionId,
+					label: extension.displayName || extension.name,
+					filter: NotificationsFilter.ERROR
+				});
+
+				this.logService.trace(`[WSO2] Filtered notifications for extension: ${extensionId}`);
+			}
+
+			this.logService.info('[WSO2] Third-party extension notification filters applied successfully.');
+		} catch (error) {
+			this.logService.error('[WSO2] Failed to apply notification filters:', error);
+		}
+	}
+
+	private isWSO2Extension(extensionId: string): boolean {
+		return WSO2ThirdPartyNotificationFilter.ALLOWED_EXT_ORGS.some(id => 
+			extensionId.toLowerCase().startsWith(`${id.toLowerCase()}.`)
+		);
+	}
+}
